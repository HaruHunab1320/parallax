{
  "name": "@parallax/pr-review-bot-demo",
  "version": "0.1.0",
  "private": true,
  "description": "Multi-agent PR review bot demonstrating Parallax orchestration",
  "scripts": {
    "start": "./run-demo.sh",
    "start:agents": "npm-run-all --parallel agent:*",
    "agent:security": "cd agents/security-agent && python agent.py",
    "agent:style": "tsx agents/style-agent/agent.ts",
    "agent:docs": "tsx agents/docs-agent/agent.ts",
    "agent:test": "cd agents/test-agent && python agent.py",
    "review": "tsx review.ts",
    "review:file": "tsx review.ts",
    "review:sample": "tsx review.ts examples/sample-code.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@parallax/sdk-typescript": "workspace:*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
