{
  "name": "pty-manager",
  "version": "1.6.3",
  "description": "PTY session manager with lifecycle management, pluggable adapters, and blocking prompt detection",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "pty-worker": "./dist/pty-worker.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./worker": {
      "require": "./dist/pty-worker.js"
    }
  },
  "files": [
    "dist",
    "scripts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:integration": "tsx test-integration.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "pty",
    "terminal",
    "pseudo-terminal",
    "node-pty",
    "session-manager",
    "shell",
    "tty",
    "process-management",
    "docker",
    "ssh",
    "bun",
    "typescript"
  ],
  "author": "Jakob Grant",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HaruHunab1320/parallax.git",
    "directory": "packages/pty-manager"
  },
  "bugs": {
    "url": "https://github.com/HaruHunab1320/parallax/issues"
  },
  "homepage": "https://github.com/HaruHunab1320/parallax/tree/main/packages/pty-manager#readme",
  "dependencies": {
    "node-pty": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.7"
  },
  "engines": {
    "node": ">=18"
  }
}
