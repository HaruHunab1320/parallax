{
  "name": "@parallax/confidence-tracker",
  "version": "1.0.0",
  "description": "Statistical confidence tracking with anomaly detection for AI agents and ML systems",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:integration": "tsx test-integration.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "confidence",
    "metrics",
    "anomaly-detection",
    "ml",
    "ai",
    "agents",
    "monitoring",
    "time-series",
    "statistics",
    "typescript"
  ],
  "author": "Parallax AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HaruHunab1320/parallax.git",
    "directory": "packages/confidence-tracker"
  },
  "bugs": {
    "url": "https://github.com/HaruHunab1320/parallax/issues"
  },
  "homepage": "https://docs.parallaxai.dev/docs/contributing/packages/confidence-tracker",
  "dependencies": {},
  "optionalDependencies": {
    "@influxdata/influxdb-client": "^1.35.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.7"
  },
  "engines": {
    "node": ">=18"
  }
}
