// Primitive: timeout
// Category: control
// Description: Enforce time limits on operations
// Confidence: zero-on-timeout


// Timeout primitive
export const timeout = (milliseconds) => {
  return (input) => {
    return {
      type: "timeout",
      input: input,
      config: {
        milliseconds: milliseconds,
        fallbackValue: null,
        includePartialResults: false,
        gracefulShutdown: true
      }
    } ~> 0.5
  }
}