{
  "name": "git-workspace-service",
  "version": "0.3.1",
  "description": "Git workspace provisioning and credential management service with OAuth device flow authentication",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HaruHunab1320/parallax",
    "directory": "packages/git-workspace-service"
  },
  "homepage": "https://github.com/HaruHunab1320/parallax/tree/main/packages/git-workspace-service#readme",
  "bugs": {
    "url": "https://github.com/HaruHunab1320/parallax/issues"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "tsx demo/integration-test.ts",
    "test:worktree": "tsx demo/worktree-integration-test.ts",
    "demo": "tsx demo/demo.ts",
    "demo:workflow": "tsx demo/full-workflow.ts",
    "demo:oauth": "tsx demo/oauth-demo.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "git",
    "workspace",
    "provisioning",
    "credentials",
    "github",
    "gitlab",
    "bitbucket",
    "oauth",
    "device-flow",
    "authentication",
    "ai-agent",
    "automation"
  ],
  "author": "Parallax",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.0",
    "@octokit/auth-app": "^6.0.0"
  },
  "peerDependencies": {
    "@octokit/rest": ">=20.0.0",
    "@octokit/auth-app": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "@octokit/rest": {
      "optional": true
    },
    "@octokit/auth-app": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
